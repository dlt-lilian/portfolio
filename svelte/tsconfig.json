{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"baseUrl": ".",
		"paths": {
			"$lib/*": ["src/lib/*"],
			"$routes/*": ["src/routes/*"]
		}
	},
	"include": ["src"],
	"exclude": ["node_modules", ".svelte-kit"]
}
